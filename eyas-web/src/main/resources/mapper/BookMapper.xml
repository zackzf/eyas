<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fbart.research.web.infrastructure.mapper.BookMapper" >

  <select id="selectByPrimaryKey" resultType="com.fbart.research.web.infrastructure.Book" parameterType="java.lang.Integer" >
